<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Cubecart UI Smoke Test Suite" verbose="1" >
    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter"></listener>
    </listeners>
    <test name="Cubecart Customer Category Module Smoke Test One" >
        <classes>
            <class name="com.unitedcoder.integrationtest.cubecarttestng.TestNGRunner"/>
        </classes>
    </test>

    <test name="Cubecart Customer Category Module Smoke Test Two" >
        <classes>
            <class name="com.unitedcoder.integrationtest.cubecarttestng.TestNGRunner" />
        </classes>
    </test>
    <test name="Cubecart Api Tests" >
        <classes>
            <class name="com.unitedcoder.integrationtest.cubecartrestapi.CubecartApiRestLibraryTest"/>
            <class name="com.unitedcoder.integrationtest.cubecarthttpapi.ApiTestRunner"></class>
        </classes>
    </test>
    <test name="Cubecart UI Database Smoke Test" >
        <classes>
            <class name="com.unitedcoder.integrationtest.database.UIDatabaseTestRunner"/>
        </classes>
    </test>

</suite>